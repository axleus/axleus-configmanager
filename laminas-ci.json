{
    "extensions": [
        "pdo-mysql"
    ],
    "ignore_php_platform_requirements": {
        "8.2": true
    },
    "stablePHP": "8.3.14",
    "additional_checks": [
        {
            "name": "PhpStan",
            "job": {
                "php": "8.3",
                "dependencies": "latest",
                "command": "composer require --dev --ignore-platform-reqs phpstan/phpstan && vendor/bin/phpstan -l 5 src analyse"
            }
        }
    ]
}